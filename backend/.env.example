# متغيرات البيئة للخادم الخلفي لتطبيق Bazaar

# بيئة التشغيل (development, production, test)
NODE_ENV=development

# إعدادات الخادم
PORT=5000
API_PREFIX=/api/v1

# إعدادات قاعدة البيانات
MONGODB_URI=mongodb://localhost:27017/bazaar

# إعدادات واجهة برمجة تطبيقات علي إكسبريس
ALIEXPRESS_API_KEY=your_api_key_here
ALIEXPRESS_API_SECRET=your_api_secret_here
ALIEXPRESS_TRACKING_ID=your_tracking_id_here
ALIEXPRESS_API_URL=https://api.aliexpress.com/v2

# نسبة هامش الربح (30%)
PROFIT_MARGIN=30

# إعدادات JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# إعدادات CORS
CORS_ORIGIN=http://localhost:3000
