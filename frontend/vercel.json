{
  "name": "bazaar-frontend",
  "version": "1.0.0",
  "private": true,
  "builds": [
    {
      "src": "package.json",{
        "name": "bazaar-frontend",
        "version": "1.0.0",
        "private": true,
        "builds": [
              {
                      "src": "package.json",
                      "use": "@vercel/next"
              }
        ],
        "routes": [
              {
                      "src": "/(.*)",
                      "dest": "/"
              }
        ],
        "rewrites": [
              { "source": "/(.*)", "destination": "/" }
        ],
        "framework": "nextjs",
        "buildCommand": "npm run build",
        "outputDirectory": ".next"
    }
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
